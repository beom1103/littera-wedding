{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "ctypto/crypto/(.*)": "<rootDir>/../libs/crypto/src/$1",
    "ctypto/crypto": "<rootDir>/../libs/crypto/src"
  }
}